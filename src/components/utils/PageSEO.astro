---
// Reas props
interface Props {
  currentPage: string; // Current page
  extraJson?: Record<string, any>;
}
const { currentPage, extraJson = {} } = Astro.props;

// Components
import BaseSEO from "./base/BaseSEO.astro";
---

<BaseSEO
  currentPage={currentPage}
  jsonType="LocalBusiness"
  extraJson={extraJson}
/>
